<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MVC_Webshop</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />


    <script src='https://kit.fontawesome.com/a076d05399.js' crossorigin='anonymous'></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">




    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/cart.css" />

    <link rel="stylesheet" href="~/css/Footer.css" />


</head>
<body class="container">
    <header>
        <nav class="navbar-container navbar navbar-expand pl-3 pr-3 pb-0">

            <div class="container-fluid">
                <div class="row w-100">

                    <div class="col">
                        <a class="navbar-brand"
                           asp-controller="Home" asp-action="Index">
                            <img src="~/img/Logo.png"
                                 width="250"
                                 class="logo" />
                        </a>
                    </div>


                    <div class="col">
                        <form class="form-inline d-flex search-bar" method="get" asp-controller="Book" asp-action="SearchResult" asp-route-SearchText="SearchText">
                            @using (Html.BeginForm("SearchResult", "search", FormMethod.Get))
                            {
                                <input id="SearchText" class="form-control mr-sm-2" type="search" name="SearchText" placeholder="search" aria-label="Search">
                                <button id="button" class="btn btn-secondary my-2 my-sm-0 m-1" type="submit">Search</button>
                            }

                        </form>
                    </div>


                    <div class="col">

                        <partial name="_LoginPartial" />

                        @*<ul class="navbar-nav icon-container">*@
                        @*<li class="nav-item">

                                <a href="#" class="nav-link">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-person-circle" viewBox="0 0 16 16">
                                        <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z" />
                                        <path fill-rule="evenodd" d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8zm8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1z" />
                                    </svg>
                                </a>
                            </li>

                            <li class="nav-item">
                                <a href="#" class="nav-link">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-cart" viewBox="0 0 16 16">
                                        <path d="M0 1.5A.5.5 0 0 1 .5 1H2a.5.5 0 0 1 .485.379L2.89 3H14.5a.5.5 0 0 1 .491.592l-1.5 8A.5.5 0 0 1 13 12H4a.5.5 0 0 1-.491-.408L2.01 3.607 1.61 2H.5a.5.5 0 0 1-.5-.5zM3.102 4l1.313 7h8.17l1.313-7H3.102zM5 12a2 2 0 1 0 0 4 2 2 0 0 0 0-4zm7 0a2 2 0 1 0 0 4 2 2 0 0 0 0-4zm-7 1a1 1 0 1 1 0 2 1 1 0 0 1 0-2zm7 0a1 1 0 1 1 0 2 1 1 0 0 1 0-2z" />
                                    </svg>
                                </a>
                            </li>*@






                        @*</ul>*@
                    </div>



                </div>
            </div>


            <div class="navbar-second-container navbar navbar-expand p-0">

                <div class="home-link">
                    <a class="fa fa-home" style="font-size:30px;color:gray"
                       asp-controller="Home" asp-action="Index">
                    </a>
                </div>

                <div class="dropdown m-1">
                    <button class="btn shadow-none dropdown-toggle disabled"
                            type="button"
                            id="dropdownMenuButton1"
                            data-bs-toggle="dropdown" aria-expanded="false">
                        Category
                    </button>
                    <ul class="dropdown-menu" aria-labelledby="">

                        <li><a class="dropdown-item" href="/Category/?Genre=Food">Food</a></li>
                        <li><a class="dropdown-item" href="/Category/?Genre=Detective">Detective</a></li>
                        <li><a class="dropdown-item" href="/Category/?Genre=Kids">Kids</a></li>


                    </ul>
                </div>

                <div>

                    @if (User.IsInRole("Admin"))
                    {
                        <p>Admin Account</p>

                        <a asp-controller="Admin" asp-action="Index">Admin pages</a>
                    }
                    @if (User.IsInRole("User"))
                    {
                        <p> User Account</p>
                    }

                </div>
                <br />
                <div class="col">
                    <i class='fas fa-shipping-fast' style='font-size:28px'></i> Free Shipping
                </div>


            </div>



        </nav>

    </header>
    <div class="content p-2">
        @RenderBody()
    </div>



    <div class="footer main-footer">
        <div class="container">
            <div class="row">

                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="widget subscribe no-box">
                        <h5 class="widget-title">BookStore<span></span></h5>
                        <p>BookStore- The Peoples Book Store to Buy Discounted Books Online</p>
                    </div>
                </div>

                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="widget no-box">
                        <h5 class="widget-title">Quick Links<span></span></h5>
                        <ul class="thumbnail-widget">
                            <li>
                                <div class="thumb-content"><a href="/" class="u-color--grey">Shop</a></div>
                            </li>
                            <li>
                                <div class="thumb-content"><a href="/Category/?Genre=Food" class="u-color--grey">Food</a></div>
                            </li>
                            <li>
                                <div class="thumb-content"><a href="/Category/?Genre=Detective" class="u-color--grey">Detective</a></div>
                            </li>
                            <li>
                                <div class="thumb-content"><a href="/Category/?Genre=Kids" class="u-color--grey">Kids</a></div>
                            </li>


                            <li>
                                <div class="thumb-content"><a href="#.">About</a></div>
                            </li>
                        </ul>
                    </div>
                </div>

                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="widget no-box">
                        <h5 class="widget-title">Get Started<span></span></h5>
                        <p>Get access to your full Training and Marketing Suite.</p>
                        <a id="subscribe-btn" class="btn">Subscribe Now</a>
                    </div>
                </div>

                <div class="col-xs-12 col-sm-6 col-md-3">

                    <div class="widget no-box">
                        <h5 class="widget-title">Contact Us<span></span></h5>

                        <p><a href="mailto:info@domain.com" title="glorythemes">info@domain.com</a></p>
                        <ul class="social-footer2">
                            <li class=""><a href="https://www.youtube.com/" target="_blank" title="youtube"><img alt="youtube" width="30" height="30" src="data:image/png;base64,"></a></li>
                            <li class=""><a href="https://www.facebook.com/" target="_blank" title="Facebook"><img alt="Facebook" width="30" height="30" src="data:image/png;base64,"></a></li>
                            <li class=""><a href="https://twitter.com" target="_blank" title="Twitter"><img alt="Twitter" width="30" height="30" src="data:image/png;base64,"></a></li>
                            <li class=""><a title="instagram" target="_blank" href="https://www.instagram.com/"><img alt="instagram" width="30" height="30" src="data:image/png;base64,"></a></li>
                        </ul>
                    </div>
                </div>

            </div>
        </div>
    </div>

    <div class="footer-copyright">
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <p>Copyright Company Name © 2022. All rights reserved.</p>
                </div>
            </div>
        </div>
    </div>






    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>



